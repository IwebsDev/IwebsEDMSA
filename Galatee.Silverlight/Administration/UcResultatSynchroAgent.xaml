        <sdk:Label x:Name="lblMiseAJour" HorizontalAlignment="Left" Height="28" Margin="53,25,0,0" VerticalAlignment="Top" Width="290" FontSize="12" FontWeight="Bold"/>
        <sdk:Label x:Name="lblModifier" HorizontalAlignment="Left" Height="28" Margin="53,57,0,0" VerticalAlignment="Top" Width="290" FontSize="12" FontWeight="Bold"/>
        <sdk:Label x:Name="lblRejeter" HorizontalAlignment="Left" Height="28" Margin="53,94,0,0" VerticalAlignment="Top" Width="290" FontSize="12" FontWeight="Bold"/>
    </Grid>
</controls:ChildWindow>