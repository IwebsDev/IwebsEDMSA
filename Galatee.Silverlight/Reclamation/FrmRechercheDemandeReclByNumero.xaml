<sdk:ChildWindow
           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
           xmlns:controls="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls"
           xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk" xmlns:Controls="clr-namespace:SilverlightContrib.Controls;assembly=SilverlightContrib.Controls" x:Class="Galatee.Silverlight.Accueil.FrmRechercheDemandeReclByNumero"
           Width="350" Height="177" 
           Title="Recherche de demande" Style="{StaticResource ChildWindowGalatee}" OverlayBrush="{x:Null}" IsTabStop="True" TabIndex="0">
    <Grid x:Name="LayoutRoot" Margin="2">
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>

        <Button x:Name="CancelButton" Content="Fermer" Click="CancelButton_Click" Width="75" Height="23" HorizontalAlignment="Right" Margin="0,12,0,0" Grid.Row="1" />
        <Button x:Name="OKButton" Content="OK" Click="OKButton_Click" Width="75" Height="23" HorizontalAlignment="Right" Margin="0,12,79,0" Grid.Row="1" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="124,27,0,0" x:Name="Txt_NumDemande" VerticalAlignment="Top" Width="190" />
        <sdk:Label Height="23" HorizontalAlignment="Left" Margin="12,31,0,0" x:Name="label1" VerticalAlignment="Top" Width="134" Content="Numéro demande" />
        <Controls:GroupBox FontWeight="SemiBold" Margin="0,-1,0,0" x:Name="groupBox1_Copy1"  />
    </Grid>
</sdk:ChildWindow>