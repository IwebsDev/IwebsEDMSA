<controls:ChildWindow
           xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
           xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
           xmlns:controls="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls"
           xmlns:Controls="clr-namespace:SilverlightContrib.Controls;assembly=SilverlightContrib.Controls" 
           xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk" 
          x:Class="Galatee.Silverlight.Accueil.FrmListeDeAction"
           Width="568" Height="237" 
           Title="FrmListeDeAction" Style="{StaticResource ChildWindowGalatee}">
    <Grid x:Name="LayoutRoot" Margin="2">
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>

        <Button x:Name="CancelButton" Content="Annuler" Click="CancelButton_Click" Width="147" Height="23" HorizontalAlignment="Right" Margin="0,12,0,0" Grid.Row="1" />
        <Button x:Name="OKButton" Content="Imprimer" Click="OKButton_Click" Width="150" Height="23" HorizontalAlignment="Right" Margin="0,12,152,0" Grid.Row="1" />
        <Controls:GroupBox FontWeight="SemiBold" Margin="0,6,0,0" x:Name="groupBox1_Copy3" Header="Critères" />
        <controls:DatePicker x:Name="dtp_DateDebut" HorizontalAlignment="Left" Height="23" Margin="113,28,0,0" VerticalAlignment="Top" Width="264"/>
        <controls:DatePicker x:Name="dtp_DateFin" HorizontalAlignment="Left" Height="23" Margin="113,58,0,0" VerticalAlignment="Top" Width="293"/>
        <sdk:Label Height="21" HorizontalAlignment="Left" Margin="5,32,0,0" x:Name="lbl_Centre_Copy3" VerticalAlignment="Top" Width="87" Content="Date debut" />
        <sdk:Label Height="19" HorizontalAlignment="Left" Margin="5,60,0,0" x:Name="lbl_Centre_Copy4" VerticalAlignment="Top" Width="108" Content="Date fin demande" />
        <sdk:Label Content="Equipe" HorizontalAlignment="Left" Height="21" Margin="10,86,0,0" VerticalAlignment="Top" Width="98"/>
        <ComboBox x:Name="cboEquipe"  HorizontalAlignment="Left" Margin="113,86,0,0" VerticalAlignment="Top" Width="258"/>
    </Grid>
</controls:ChildWindow>